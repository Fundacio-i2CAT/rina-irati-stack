ccflags-y += -I$..
ccflags-y += -I$../vmpi
ccflags-y += -Wtype-limits

obj-m  += normal-ipcp.o
obj-m  += shim-eth-vlan.o
shim-eth-vlan-y :=				\
	shim-eth-vlan-core.o shim-eth-utils.o
obj-m   += shim-tcp-udp.o

ifeq ($(HAVE_VMPI),y)
obj-m   += shim-hv.o
shim-hv-y := shim-hv-core.o vmpi-bufs.o
endif
