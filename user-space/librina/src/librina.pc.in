prefix=@prefix@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: LibRINA
Description: RINA support libraries
Version: @PACKAGE_VERSION@
URL: http://irati.eu
Requires: libnl-3.0 >= 3.1, libnl-genl-3.0 >= 3.1
#Conflicts: foobar <= 4.5
Libs: @LIBNL3_LIBS@ @LIBNLGENL3_LIBS@
Libs.private: -lpthread
Cflags: -I${includedir} @LIBNL3_CFLAGS@ @LIBNLGENL3_CFLAGS@

